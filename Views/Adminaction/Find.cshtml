@{
    ViewData["Title"] = "Welcome";
}
<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="~/css/index.css">
    <script src="~/js/adminAction.js"></script>
</head>
<body>
    <div>
    </div>
    <h1>查看已发布游戏</h1>
    @for (int i = 0; i < (ViewData["List"] as List<data>).Count(); i++)
    {

        List<data> List = ViewData["List"] as List<data>;

        <table class="table table-bordered" style="margin-top:10px;">
            <tr>
                <th style="text-aligin:left;">Commodity_ID ：</th>
                <td>
                    @List[i].ID
                </td>
                <th style="text-aligin:left;">Commodity_Name：</th>
                <td>
                    @List[i].Name
                </td>
            </tr>

            <tr>
                <th style="text-aligin:left;">Publisher：</th>
                <td>
                    @List[i].Publisher
                </td>
                <th style="text-aligin:left;">Price：</th>
                <td>
                    @List[i].Price
                </td>
            </tr>

            <tr>
                <th style="text-aligin:left;">Publish_Time：</th>
                <td>
                    @List[i].Time
                </td>
                <th style="text-aligin:left;">Classification：</th>
                <td>
                    @List[i].Classification
                </td>
            </tr>
            <tr>
                <th style="text-aligin:left;">DownloadURL：</th>
                <td>
                    @List[i].Downloadurl
                </td>
                <th style="text-aligin:left;">SalesVolume：</th>
                <td>
                    @List[i].Volume
                </td>
            </tr>
            <tr>
                <th style="text-aligin:left;">Description：</th>
                <td colspan="2">@List[i].Description</td>
            </tr>
            <tr>
                <th colspan="4" style="text-align:center;">
                    <input type="button" class="btn btn-default" onclick="    javascript: window.history.back();" value="确认" />
                    <input type="button" class="btn btn-default" onclick="    javascript: window.history.back();" value="返回" />
                </th>
            </tr>
        </table>
    }

</body>
</html>